class Solution {
public:
     int evalRPN(vector<string>& tokens) {
     stack<int> st;
        for (string ch : tokens) {
            if (ch != "+" && ch != "-" && ch != "*" && ch != "/") {
                st.push(stoi(ch)); 
            } else {
                int b = st.top(); st.pop();
                int a = st.top(); st.pop();
                int res = 0;
                
                if (ch == "+") res = a + b;
                else if (ch == "-") res = a - b;
                else if (ch == "*") res = a * b;
                else if (ch == "/") res = a / b; 
                
                st.push(res);
            }
        }
        return st.top();
    }
};
